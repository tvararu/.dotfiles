Xft.dpi:       96
Xft.antialias: true
Xft.hinting:   true
Xft.rgba:      rgb
Xft.autohint:  false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault

*.font: monospace:size=11
urxvt*font: xft:fira code:size=11

! Custom
*.foreground:    #FFFFFF
*.background:    #1E2127
*.cursorColor:   #5C6370
*.highlightColor:#3A3F4B

! Black + DarkGrey
*.color0:        #1E2127
*.color8:        #5C6370

! DarkRed + Red
*.color1:        #E06C75
*.color9:        #E06C75

! DarkGreen + Green
*.color2:        #98C379
*.color10:       #98C379

! DarkYellow + Yellow
*.color3:        #D19A66
*.color11:       #D19A66

! DarkBlue + Blue
*.color4:        #61AFEF
*.color12:       #61AFEF

! DarkMagenta + Magenta
*.color5:        #C678DD
*.color13:       #C678DD

! DarkCyan + Cyan
*.color6:        #56B6C2
*.color14:       #56B6C2

! LightGrey + White
*.color7:        #ABB2BF
*.color15:       #FFFFFF
